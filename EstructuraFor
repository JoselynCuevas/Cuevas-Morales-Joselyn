print ("1. Ejercicio")
for j in range (1,8):
    print('*' * j)

print ("2. Ejercicio")
for k in range (8, 0, -1):
    print ('*' * k)

print ("3. Ejercicio")
for l in range (1,8):
    print '*' * l)
for m in range (8, 0, -1):
    print '*' * m)

print ("4. Ejercicio")
for n in range (1,8):
   print  ('*' * 8)
